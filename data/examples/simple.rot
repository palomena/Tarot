function boo() -> List[Integer] {
	let x =  [1,2,3,4,5,6,7];
	x.append(10/2);
	return x;
}

function main() {
	let sum = Integer(0);
	let i   = Integer(0);
	let list = boo();
	while i < list.length {
		sum += list[i];
		i += 1;
	}
	println(f"Sum: {abs(sum)}");
}

